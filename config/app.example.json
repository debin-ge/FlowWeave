{
  "log_level": "info",
  "log_format": "text",
  "server": {
    "host": "0.0.0.0",
    "port": 8080,
    "read_timeout_seconds": 30,
    "write_timeout_seconds": 600
  },
  "database": {
    "url": "postgres://user:password@localhost:5432/flowweave?sslmode=disable",
    "max_open_conns": 25,
    "max_idle_conns": 5,
    "conn_max_lifetime_seconds": 300
  },
  "redis": {
    "url": "redis://localhost:6379/0"
  },
  "engine": {
    "max_workers": 4,
    "node_timeout_seconds": 300,
    "max_node_steps": 100
  },
  "auth": {
    "jwt_secret": "",
    "jwt_issuer": ""
  },
  "openai": {
    "api_key": "",
    "base_url": "https://api.openai.com/v1"
  },
  "summary": {
    "provider": "openai",
    "model": "gpt-4o-mini"
  },
  "gateway": {
    "provider": "openai",
    "model": "gpt-4o-mini",
    "context_window_size": 128000,
    "threshold_ratio": 0.7,
    "min_recent_turns": 4
  },
  "rag": {
    "opensearch_url": "https://localhost:9200",
    "opensearch_username": "admin",
    "opensearch_password": "admin",
    "index_prefix": "rag",
    "default_mode": "bm25",
    "enable_rerank": false,
    "default_top_k": 5,
    "embedding_provider": "openai",
    "embedding_model": "text-embedding-3-small",
    "embedding_dims": 1536,
    "rerank_provider": "openai",
    "rerank_model": "gpt-4o-mini",
    "chunk_size": 512,
    "chunk_overlap": 128,
    "cache_ttl": 300,
    "max_file_size": 50
  }
}
